<application>
  <component name="KeyPromoterXStatistic">
    <option name="statistics">
      <Statistic Action="Build Project">
        <Item>
          <IdeaActionID>CompileDirty</IdeaActionID>
          <ShortCut>'Ctrl+F9'</ShortCut>
          <Description>Build Project</Description>
          <Count>3</Count>
        </Item>
      </Statistic>
      <Statistic Action="Copy">
        <Item>
          <IdeaActionID>$Copy</IdeaActionID>
          <ShortCut>'Ctrl+C' or 'Ctrl+Insert'</ShortCut>
          <Description>Copy</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="Cut">
        <Item>
          <IdeaActionID>$Cut</IdeaActionID>
          <ShortCut>'Ctrl+X' or 'Shift+Delete'</ShortCut>
          <Description>Cut</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="New Folder...">
        <Item>
          <IdeaActionID>FileChooser.NewFolder</IdeaActionID>
          <ShortCut>'Alt+Insert' or 'Ctrl+N'</ShortCut>
          <Description>New Folder...</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="Project Directory">
        <Item>
          <IdeaActionID>FileChooser.GotoProject</IdeaActionID>
          <ShortCut>'Ctrl+2'</ShortCut>
          <Description>Project Directory</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="Settings...">
        <Item>
          <IdeaActionID>ShowSettings</IdeaActionID>
          <ShortCut>'Ctrl+Alt+S'</ShortCut>
          <Description>Settings...</Description>
          <Count>3</Count>
        </Item>
      </Statistic>
    </option>
  </component>
</application>